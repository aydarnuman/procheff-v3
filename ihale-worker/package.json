{
  "name": "ihale-worker",
  "version": "1.0.0",
  "description": "Playwright-based scraper for Ihalebul.com",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:clean": "./start-clean.sh",
    "build": "tsc",
    "start": "node dist/server.js",
    "kill": "pkill -f 'tsx.*server.ts' || lsof -ti:8080 | xargs kill -9"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "json2csv": "^6.0.0-alpha.2",
    "playwright": "^1.47.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/json2csv": "^5.0.7",
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
